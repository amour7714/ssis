          SQLTask:Connection="{4D5FC494-C056-41D7-AEF3-9C28964CE0D8}"
          SQLTask:SqlStatementSource="-- set all single-row business keys to have EffectiveDate=1900-01-01 so that back-dated &#xA;-- transactional data can still join to dimension row between EffectiveDate and ExpiryDate&#xA;&#xA;update d&#xA;&#x9;set EffectiveDate = '1900-01-01'&#xA;--select d.*&#xA;from conformed.DimReferenceCode d&#xA;join (&#xA;&#x9;select s1.ReferenceCode, s1.CompanyCode&#xA;&#x9;from conformed.DimReferenceCode s1&#xA;&#x9;where s1.EffectiveDate &gt; '1900-01-01'&#xA;&#x9;&#x9;and s1.ReferenceCode + s1.CompanyCode not in (select s2.ReferenceCode + s2.CompanyCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;from conformed.DimReferenceCode s2&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;group by s2.ReferenceCode, s2.CompanyCode&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;having count(*) &gt; 1)&#xA;&#x9;group by s1.ReferenceCode, s1.CompanyCode&#xA;&#x9;having count(*) = 1&#xA;) q on d.ReferenceCode = q.ReferenceCode and d.CompanyCode = q.CompanyCode" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
