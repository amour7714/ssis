              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">declare @today_less_35 datetime = convert(date, getdate() - 35)

SELECT  o.division_id,
	d.branch_code,
	wb.posted_date,
	p.customer_id,
	p.customer_name, 
	--p.mill_name,
	p.waybill,
	p.waybill_date, 
	p.waybill_id,
	p.sales_rep_id,
	p.sales_rep,
	wb.vessel_number,
	wb.po_number,
	wb.other_reference, 
	p.master_product, 
	p.variable_item, 
	ROUND(SUM(p.[weight]),0) as [weight], 
	w.note, 
	p.[length]/304.8 as [length], 
	0 as wt_per_m,
	SUM(p.quantity) as units, 
	SUM(p.number_of_items) as pieces,
	CAST(SUM(p.quantity) as varchar(5)) + '/' + p.variable_item  as unittally, 
	CAST(SUM(p.number_of_items) as varchar(6)) + '/' + p.variable_item as piecetally 
FROM inventory.v_proddtl p
JOIN inventory.v_outbound_waybill w on p.waybill_id = w.waybill_id 
JOIN inventory.waybill wb on w.waybill_id = wb.waybill_id 
JOIN inventory.v_shipping_order o on w.order_id = o.order_id 
JOIN rdata.division d on d.division_id = o.division_id
WHERE wb.posted_date &gt;= @today_less_35
GROUP BY o.division_id, d.branch_code, wb.posted_date, p.customer_id,p.customer_name, --p.mill_name, 
                    p.waybill, p.waybill_date, p.waybill_id, p.sales_rep_id, p.sales_rep, master_product, 
                    variable_item, w.note, [length] / 304.8, wb.vessel_number, wb.po_number, wb.other_reference
ORDER BY p.customer_name, p.waybill, master_product, [length]</property>
